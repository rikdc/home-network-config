{
  "mqtt": {
    "name": "homeassistant",
    "host": "192.168.10.8",
    "port": 1883,
    "qos": 0,
    "prefix": "homeassistant",
    "reconnectPeriod": 5000,
    "retain": true,
    "clean": true,
    "auth": false,
    "_ca": "",
    "ca": "",
    "_cert": "",
    "cert": "",
    "_key": "",
    "key": "",
    "store": true,
    "disabled": true
  },
  "gateway": {
    "type": 0,
    "plugins": [
      "@varet/zj2m-prom-exporter"
    ],
    "authEnabled": false,
    "payloadType": 1,
    "nodeNames": true,
    "hassDiscovery": false,
    "discoveryPrefix": "homeassistant",
    "logEnabled": false,
    "logLevel": "debug",
