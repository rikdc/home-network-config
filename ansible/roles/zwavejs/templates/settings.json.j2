  "zwave": {
    "port": "/dev     /ttyACM0",
    "commandsTimeout": 15000,
    "logLevel": "debug",
    "logEnabled": true,
    "securityKeys": {
      "S0_Legacy": "{{ zwavejs_keys_s0_legacy }}",
