    "serverEnabled": true,
    "serverServiceDiscoveryDisabled": false,
    "enableSoftReset": true,
    "enableStatistics": true,
    "serverPort": 3000,
    "maxNodeEventsQueueSize": 100,
    "logging": true,
    "saveConfig": true,
    "pollInterval": 5000,
    "disclaimerVersion": 1
  },
  "backup": {
    "storeBackup": false,
    "storeCron": "0 0 * * *",
